{
  "name": "jira-ci-cd-integration",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "prebuild": "npm run lint && npm run test",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix ",
    "prepackage": "npm run build",
    "package": "npm run package:action",
    "package:action": "ncc build lib/github/main.js --source-map --license licenses.txt",
    "test": "jest"
  },
  "engines": {
    "node": "12.16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit-gohri/jira-ci-cd-integration.git"
  },
  "keywords": [
    "actions",
    "node",
    "integration",
    "jira"
  ],
  "author": "Rohit Gohri <work@rohit.page>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/webhooks-definitions": "^3.67.3",
    "isomorphic-fetch": "^3.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.5",
    "jest-circus": "^27.0.5",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
