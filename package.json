{
  "name": "jira-ci-cd-integration",
  "version": "0.2.10",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc --noEmit",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix ",
    "package": "npm run package:docker && npm run package:action",
    "package:docker": "ncc build src/docker/main.ts --out dist/docker --source-map --license licenses.txt",
    "package:action": "ncc build src/github/main.ts --out dist/github --source-map --license licenses.txt",
    "increment": "npm --no-git-tag-version version",
    "test": "jest"
  },
  "engines": {
    "node": "12.16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit-gohri/jira-ci-cd-integration.git"
  },
  "keywords": [
    "actions",
    "node",
    "integration",
    "jira"
  ],
  "author": "Rohit Gohri <work@rohit.page>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/webhooks-definitions": "^3.67.3",
    "@relative-ci/env-ci": "^5.3.1",
    "isomorphic-fetch": "^3.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/env-ci": "^3.1.1",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.5",
    "jest-circus": "^27.0.5",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
